        <!-- Customize your light theme here. -->
        <item name="colorPrimary">#5000FF</item>
        <item name="colorPrimaryVariant">#264DDD</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
